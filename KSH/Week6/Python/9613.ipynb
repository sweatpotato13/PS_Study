   "source": [
    "import itertools\n",
    "def gcd(a, b):\n",
    "    return gcd(b, a % b) if b else a\n",
    "\n",
    "t = int(input())\n",
    "for i in range(t):\n",
    "    num_list = list(map(int, input().split()))\n",
    "    num_list = num_list[1:]\n",
    "    ans = 0\n",
    "    for a, b in itertools.combinations(num_list, 2):\n",
    "        ans += gcd(a, b)\n",
    "    print(ans)"
   ]
  }
 ]
