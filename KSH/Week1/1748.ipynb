    "##1748\n",
    "\n",
    "n = '120'\n",
    "nlen = len(n) - 1\n",
    "c = 0\n",
    "i = 0\n",
    "while i < nlen:\n",
    "    c += 9 * (10 ** i) * (i + 1)\n",
    "    i += 1\n",
    "c += ((int(n) - (10 ** nlen)) + 1) * (nlen + 1)\n",
    "print(c)"
   ]
