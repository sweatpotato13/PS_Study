   "source": [
    "n = int(input())\n",
    "arr = sorted(map(int, input().split()))\n",
    "x = int(input())\n",
    " \n",
    "cnt = 0\n",
    " \n",
    "i = 0\n",
    "j = n-1\n",
    " \n",
    "while i != j :\n",
    "    s = arr[i] + arr[j]\n",
    "    if s == x :\n",
    "        cnt += 1\n",
    "        i += 1\n",
    "    elif s > x :\n",
    "        j -= 1\n",
    "    else :\n",
    "        i += 1\n",
    " \n",
    "print(cnt)"
   ]
  }
