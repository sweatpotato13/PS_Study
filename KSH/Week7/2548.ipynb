    "num_list = list(map(int, input().split()))\n",
    "\n",
    "ans=100000000000000000\n",
    "wt=200000\n",
    "for i in num_list:\n",
    "    diff=0\n",
    "    for j in num_list:\n",
    "        diff+=abs(j-i)\n",
    "    if diff<=ans:\n",
    "        wt=min(wt,i)\n",
    "    ans=min(ans,diff)\n",
    "print(wt)"
