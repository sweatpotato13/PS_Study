   "source": [
    "import itertools\n",
    "cnt=int(input())\n",
    "pool = list(map(int, input().split()))\n",
    "pool.sort()\n",
    "pool2 = list(itertools.permutations(pool))\n",
    "ans=0\n",
    "for i in pool2:\n",
    "    diff=0\n",
    "    for j in range(1,cnt):\n",
    "        diff+=abs(i[j]-i[j-1])\n",
    "    ans=max(ans,diff)\n",
    "\n",
    "print(ans)"
   ]
